<script src="/socket.io/socket.io.js"></script>
<script>
    function start(){
    $(function () {
        var socket = io.connect();
        
            socket.emit('json update');
        $(document).ready(function () {
            $.getJSON("processData.json", function (data) {
                $(data).each(function (index, value) {
                    $("#schema").append("L'id du schema est : " + value.schemaID);
                    $("#collab").append(value.Processus.Steps[0].step.collaborateurs);
                })
            })
        })
    })}
</script>
